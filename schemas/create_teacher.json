{
  "type": "object",
  "required": [
    "username",
    "password",
    "email"
  ],
  "properties": {
    "username": {
      "type": "string"
    },
    "password": {
      "type": "string"
    },
    "email": {
      "type": "string",
      "pattern": "^[a-z][a-z0-9_\\.]{5,32}@[a-z0-9]{2,}(\\.[a-z0-9]{2,4}){1,2}$",
      "minLength": 11
    }
  },
  "additionalProperties": false
}