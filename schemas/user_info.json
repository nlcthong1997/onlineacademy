{
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "email": {
      "type": "string",
      "pattern": "^[a-z][a-z0-9_\\.]{5,32}@[a-z0-9]{2,}(\\.[a-z0-9]{2,4}){1,2}$",
      "minLength": 11
    },
    "full_name": {
      "type": "string",
      "minLength": 10
    },
    "address": {
      "type": "string"
    },
    "phone": {
      "type": "string",
      "maxLength": 11
    },
    "img": {
      "type": "string"
    }
  }
}